<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.lab_el">
    <uses-permission android:name="android.permission.WAKE_LOCK" />
    <application
        android:allowBackup="true" android:icon="@drawable/ic_launcher_background"
        android:label="@string/app_name" android:theme="@style/Theme.AppCompat.DayNight">
        <activity
            android:name=".MainActivity" android:label="@string/app_name"
            tools:ignore="MissingClass"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity><service
        android:name=".AlarmService" android:enabled="true"
        tools:ignore="MissingClass" />
        <receiver android:name=".AlarmReceiver"
            tools:ignore="MissingClass" />
    </application>
</manifest>
